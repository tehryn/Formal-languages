void vypis(double x[], double y[][10], int k,int m)

{

int i,j ;

for(i = 0; i<k;i++) printf("%lf\n",x[i]);

 

for(i = 0; i<m;i++)

   for(i = 0; i<10; i++)

       printf("%lf\n",y[i][j]);

}

void vypis_point(double * x, double **y, int k,int m, int n)

{

int i,j ;

for(i = 0; i<k;i++) printf("%lf\n",x[i]);

 

for(i = 0; i<m;i++)

   for(i = 0; i<n; i++)

       printf("%lf\n",y[i][j]);

}
